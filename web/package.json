{
  "name": "web",
  "version": "0.0.1",
  "scripts": {
    "dev": "concurrently \"go run main.go\" \"npm run client:dev\"",
    "client:dev": "vite",
    "server:dev": "ts-node-dev --project tsconfig.server.json src/server/server",
    "server:build": "tsc --project tsconfig.server.json",
    "client:build": "vite build",
    "storybook:build": "build-storybook -o dist/storybook",
    "build": "npm run server:build && npm run client:build && npm run storybook:build",
    "serve": "vite preview",
    "prepare": "cd .. && husky install",
    "test": "tsc && prettier --check . && eslint . && stylelint \"**/*.css\"",
    "storybook": "start-storybook -p 6006",
    "start": "go run main.go"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@storybook/addon-actions": "^6.5.0-beta.1",
    "@storybook/addon-essentials": "^6.5.0-beta.1",
    "@storybook/addon-links": "^6.5.0-beta.1",
    "@storybook/builder-vite": "^0.1.33",
    "@storybook/react": "^6.5.0-beta.1",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.1.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.5.11",
    "http-proxy-middleware": "^2.0.6",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "prettier": "2.6.2",
    "stylelint": "^14.8.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.7"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0"
  },
  "description": "> Quickly bootstrap a new project with Vite Boilerplate.",
  "main": "vite.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zapponejosh/jellyfish.git"
  },
